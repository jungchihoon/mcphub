{
  "mcpServers": {
    "context7": {
      "type": "stdio",
      "enabled": true,
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "mcp-atlassian-jira": {
      "type": "streamable-http",
      "url": "https://mcp-jira-service.livelybeach-90f399a8.koreacentral.azurecontainerapps.io/mcp/",
      "headers": {
        "Authorization": "Bearer ${USER_ATLASSIAN_JIRA_TOKEN}",
        "X-User-Email": "${USER_ATLASSIAN_JIRA_EMAIL}",
        "X-Jira-URL": "${USER_ATLASSIAN_JIRA_CLOUD_ID}"
      }
    },
    "mcp-atlassian-confluence": {
      "type": "streamable-http",
      "url": "https://mcp-confluence-service.livelybeach-90f399a8.koreacentral.azurecontainerapps.io/mcp/",
      "headers": {
        "Authorization": "Bearer ${USER_ATLASSIAN_CONFLUENCE_TOKEN}",
        "X-User-Email": "${USER_ATLASSIAN_CONFLUENCE_EMAIL}",
        "X-Jira-URL": "${USER_ATLASSIAN_CONFLUENCE_CLOUD_ID}"
      }
    },
    "github-pr-mcp-server": {
      "type": "streamable-http",
      "url": "https://github-pr-mcp-server.livelybeach-90f399a8.koreacentral.azurecontainerapps.io/mcp/",
      "enabled": true,
      "headers": {
        "Authorization": "Bearer ${USER_GITHUB_TOKEN}"
      },
      "tools": {
        "GitHub PR MCP (ACA)-create_pull_request": {
          "enabled": true
        }
      }
    }
  }
}